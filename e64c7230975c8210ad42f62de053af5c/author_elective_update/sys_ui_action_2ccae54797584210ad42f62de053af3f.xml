        <client_script_v2><![CDATA[function onClick(g_form) {
	var hospital_name = g_form.getValue('hospital');
	if(hospital_name == ""){
		//g_form.addErrorMessage('Please fill the hospital');
		g_form.showFieldMsg("hospital", "Please fill the hospital", "error");
	}
	var doc_name = g_form.getValue('doctor');
	if(doc_name == ""){
		//g_form.addErrorMessage('Please fill the doctor');
		g_form.showFieldMsg("doctor", "Please fill the doctor", "error");
	}
	var appointment_date = g_form.getValue('appointment_date');
